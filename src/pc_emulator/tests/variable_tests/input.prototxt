config_name: "DataType_Test_Configuration"
log_level: LOG_INFO
enable_kronos: false
run_time_secs: 1
config_global_pou_var {
    name: "__CONFIG_GLOBAL__"
    datatype_field {
        field_name: "global_bool_var"
        field_datatype_name: "BOOL"
        intf_type : VAR_EXPLICIT_STORAGE
        field_storage_spec {
            mem_type: RAM_MEM
            byte_offset: 3
            bit_offset: 1
        }
    }
    datatype_field {
        field_name: "global_int_var"
        field_datatype_name: "INT"
        intf_type : VAR_EXPLICIT_STORAGE
        field_storage_spec {
            full_storage_spec: "%MW4"
        }
    }
    datatype_field {
        field_name: "global_bool_arr"
        field_datatype_name: "BOOL"
        dimension_1: 10
        intf_type : VAR_EXPLICIT_STORAGE
        field_storage_spec {
            full_storage_spec: "%MW10"
        }
    }
    datatype_field {
        field_name: "global_int_arr"
        field_datatype_name: "INT"
        dimension_1: 2
        dimension_2: 2
        intf_type : VAR_EXPLICIT_STORAGE
        field_storage_spec {
            full_storage_spec: "%MW20"
        }
    }
    datatype_field {
        field_name: "start"
        field_datatype_name: "BOOL"
    }
    datatype_field {
        field_name: "start_time"
        field_datatype_name: "TIME"
    }
}
machine_spec {
    num_cpus: 1
    ram_mem_size_bytes: 10000
    resource_spec {
        resource_name: "CPU_001"
        input_mem_size_bytes: 10000
        output_mem_size_bytes: 10000
        pou_var {
            name: "PROGRAM_1"
            pou_type: PROGRAM
            datatype_field {
                field_name: "input_1"
                field_datatype_name: "INT"
                intf_type: VAR_INPUT
            }
            datatype_field {
                field_name: "input_2"
                field_datatype_name: "INT"
                intf_type: VAR_INPUT
            }
            datatype_field {
                field_name: "output_1"
                field_datatype_name: "REAL"
                intf_type: VAR_OUTPUT
            }
            datatype_field {
                field_name: "output_2"
                field_datatype_name: "REAL"
                intf_type: VAR_OUTPUT
            }
            datatype_field {
                field_name: "inout_1"
                field_datatype_name: "INT"
                intf_type: VAR_IN_OUT
            }
            datatype_field {
                field_name: "inout_2"
                field_datatype_name: "INT"
                intf_type: VAR_IN_OUT
            }
            datatype_field {
                field_name: "var_1"
                field_datatype_name: "INT"
                intf_type: VAR
            }
            datatype_field {
                field_name: "drv_1"
                field_datatype_name: "INT"
                intf_type: VAR_EXPLICIT_STORAGE
                field_storage_spec {
                    full_storage_spec: "%MW10"
                }
            }
            datatype_field {
                field_name: "global_int_arr"
                field_datatype_name: "INT"
                intf_type: VAR_EXTERNAL
            }
            datatype_field {
                field_name: "start"
                field_datatype_name: "BOOL"
                intf_type: VAR_EXTERNAL
            }
        }
    }
}